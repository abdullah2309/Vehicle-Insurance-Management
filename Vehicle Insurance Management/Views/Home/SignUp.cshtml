<!-- Login Section -->
<section class="login-section style-two">
    <div class="auto-container">
        <div class="login-box">
            <div class="section-icons-outer parallax-scene-2">
                <div data-depth="0.20" class="icon-layer-one" style="background-image: url(images/icons/icon-1.png)">
                </div>
                <div data-depth="0.40" class="icon-layer-two"
                     style="background-image: url(images/background/pattern-4.png)"></div>
            </div>
            <!-- Sec Title -->
            <div class="title-box">
                <h2>Sign Up</h2>
      
            </div>

            @if (TempData["already"] != null)
            {
                <div class="alert alert-success">@TempData["already"]</div>

                <script>
                    alert("@TempData["already"]");
                </script>
            }
            @if (TempData["ErrorMessage"] != null)
            {
                <div class="alert alert-danger">@TempData["ErrorMessage"]</div>

                <script>
                    alert("@TempData["ErrorMessage"]");
                </script>
            }
            <!-- Form Box -->
            <div class="form-box">
                <!-- Login Form -->
                <div class="styled-form">
                    <form method="post" asp-action="SignUp">
                        <div class="form-group">
                            <label>Enter your User Name</label>
                            <span class="icon flaticon-envelope"></span>
                            <input type="text" asp-for="User_Name"   placeholder="Enter Your User Name" required />
                        </div>
                        <div class="form-group">
                            <label>Enter your Email</label>
                            <input type="text" asp-for="User_Email"  placeholder="Enter Your User Email " required />
                            <span class="icon flaticon-envelope"></span>
                            <span id="red" asp-validation-for="User_Email"></span>
                        </div>
                        <div class="form-group">
                            <label>Enter your Password</label>
                            <span class="icon flaticon-padlock-1"></span>
                            <input type="password" asp-for="User_Password"  placeholder="Password" required />
                            <span class="eye-icon flaticon-eye"></span>
                            <span id="red" asp-validation-for="User_Password"></span>
                        </div>
                      
                        <div class="form-group">
                            <div class="check-box">
                                <input type="checkbox" name="remember-password" id="type-1" />
                               
                            </div>
                        </div>
                        <div class="form-group text-center">
                            <button type="submit" class="theme-btn btn-style-one">
                                <span class="txt">Sign Up</span>
                            </button>
                        </div>
                        <div class="form-group">
                            <div class="users">
                                already have account? <a asp-action="Login">login in</a>
                            </div>
                        </div>
                    </form>
                </div>
            </div>


        </div>
    </div>
</section>
<!-- End Login Section -->